<ul>
    <% @universities_programs.each do |program|%>
    <li>
        <h3>University:</h3>
        <%= program.university.name %>
        <h3>Program:</h3>
        <%= program.program.name%>
        <h3>Degree:</h3>
        <%=program.degree%>
        <%= link_to "universities_programs/#{program.id}" do %>
            <h2>Visit</h2>
        <%end%>
    </li>
    <%end%>
</ul>